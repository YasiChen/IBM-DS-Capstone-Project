    "Postal_Codes=[]\n",
    "Boroughs=[]\n",
    "Neighbourhoods=[]\n",
    "\n",
    "\n",
    "rows = tables.find_all('tr')\n",
    "\n",
    "for row in rows:\n",
    "    cells = row.find_all('td')\n",
    "#   the cell start from the 2nd section\n",
    "    if len(cells)>1:\n",
    "        if not cells[1].text.strip()=='Not assigned':\n",
